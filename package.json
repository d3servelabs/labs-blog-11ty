{
  "name": "eleventy-i18n",
  "version": "2.0.0",
  "description": "Minimal starter for localized content, using Eleventy's own Internationalization (I18n) plugin",
  "license": "ISC",
  "type": "module",
  "author": {
    "name": "Lene Saile",
    "url": "https://www.lenesaile.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-i18n.git"
  },
  "engines": {
    "node": ">=18.x.x"
  },
  "scripts": {
