    "commander": "^11.0.0",
    "dotenv": "^17.0.1",
    "glob": "^10.3.0",
    "gray-matter": "^4.0.3",
    "openai": "^5.8.2",
    "p-limit": "^4.0.0",
    "puppeteer": "^24.11.1"
  }
